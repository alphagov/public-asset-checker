{
    "name": "govuk-public-asset-checker",
    "repository": "https://github.com/alphagov/public-asset-checker",
    "env": {
      "RAILS_SERVE_STATIC_ASSETS": {
        "value": "yes"
      },
      "SECRET_KEY_BASE": {
        "generator": "secret"
      },
      "HEROKU_APP_NAME": {
        "required": true
      }
    },
    "image": "heroku/ruby",
    "buildpacks": [
      {
        "url": "https://github.com/heroku/heroku-buildpack-ruby"
      }
    ],
    "addons": []
  }